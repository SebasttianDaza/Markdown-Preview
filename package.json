    "eslint": "~8.17.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-prettier": "~4.0.0",
    "eslint-plugin-react": "~7.30.0",
    "eslint-plugin-react-hooks": "~4.5.0",
    "npm-check-updates": "latest",
